{
  "name": "smart-calendar-proxy",
  "version": "1.0.0",
  "exports": "./app.js",
  "scripts": {
    "test": "jest",
    "build": "rm -fr dist && tsc --build tsconfig.build.json",
    "start": "node dist/app.js",
    "dev": "nodemon --exec \"cross-env NODE_ENV=development node --no-warnings=ExperimentalWarning --loader ts-node/esm\" src/app.ts",
    "migration:generate": "cross-env NODE_ENV=development typeorm-ts-node-esm migration:generate ./src/migrations/migration -d ./src/dataSource.ts",
    "migration:dev": "cross-env NODE_ENV=development typeorm-ts-node-esm migration:run -d ./src/dataSource.ts",
    "migration:revert:dev": "cross-env NODE_ENV=development typeorm-ts-node-esm migration:revert -d ./src/dataSource.ts",
    "migration:prod": "typeorm migration:run -d ./dist/dataSource.js",
    "migration:revert:prod": "migration:run migration:revert -d ./dist/dataSource.js"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "axios": "^1.7.8",
    "axios-cookiejar-support": "^5.0.5",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "express-jwt": "^8.5.1",
    "express-validator": "^7.2.1",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tough-cookie": "^5.1.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
